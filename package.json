{
  "name": "data-cleaner",
  "version": "0.0.0-managed-by-semantic-release",
  "description": "Django-forms-inspired object data cleaner and validator for Node.js",
  "main": "dist/bundle.umd.js",
  "types": "dist/index.d.ts",
  "source": "src/index.ts",
  "files": [
    "src/**",
    "dist/**"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/IlyaSemenov/node-data-cleaner.git"
  },
  "keywords": [
    "validation"
  ],
  "author": "Ilya Semenov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/IlyaSemenov/node-data-cleaner/issues"
  },
  "homepage": "https://github.com/IlyaSemenov/node-data-cleaner#readme",
  "devDependencies": {
    "@commitlint/cli": "^7.2.1",
    "@commitlint/config-conventional": "^7.1.2",
    "chai": "^4.1.1",
    "chai-as-promised": "^7.1.1",
    "eslint": "^5.10.0",
    "eslint-plugin-typescript": "^0.14.0",
    "husky": "^1.2.1",
    "lint-staged": "^8.1.0",
    "mocha": "^3.5.0",
    "prettier-eslint-cli": "^4.7.1",
    "rollup": "^0.59.1",
    "rollup-plugin-cleaner": "^0.2.0",
    "rollup-plugin-typescript2": "^0.14.0",
    "semantic-release": "^15.9.17",
    "typescript": "^2.8.3",
    "typescript-eslint-parser": "^21.0.2"
  },
  "scripts": {
    "test": "mocha",
    "build": "rollup -c",
    "prepublish": "npm run build",
    "lint": "prettier-eslint --write '{src,test}/**/*.{js,ts}'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "prettier-eslint --write",
      "git add"
    ]
  },
  "commitlint": {
    "extends": ["@commitlint/config-conventional"]
  }
}
