{
  "name": "data-cleaner",
  "version": "0.0.0-managed-by-semantic-release",
  "description": "Django-forms-inspired object data cleaner and validator for Node.js",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "source": "src/index.ts",
  "files": [
    "src/**",
    "dist/**"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/IlyaSemenov/node-data-cleaner.git"
  },
  "keywords": [
    "validation"
  ],
  "author": "Ilya Semenov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/IlyaSemenov/node-data-cleaner/issues"
  },
  "homepage": "https://github.com/IlyaSemenov/node-data-cleaner#readme",
  "dependencies": {
    "capital-case": "^1.0.3",
    "email-validator": "^2.0.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.0",
    "prettier": "^2.1.2",
    "rollup": "^2.29.0",
    "rollup-plugin-cleaner": "^1.0.0",
    "rollup-plugin-typescript2": "^0.27.3",
    "semantic-release": "^17.1.2",
    "tap": "^14.10.8",
    "typescript": "^4.0.3"
  },
  "scripts": {
    "test": "tap --jobs-auto --no-coverage 'test/**/*.test.js'",
    "build": "rollup -c",
    "prepublish": "npm run build",
    "lint": "eslint --fix '**/*.{js,ts}'"
  },
  "prettier": {
    "useTabs": true,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
